{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AlphaMindEmissionsReadOnly",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:GetObjectVersion"
            ],
            "Resource": [
                "arn:aws:s3:::alphamind-emissions-data/*"
            ]
        },
        {
            "Sid": "AlphaMindEmissionsListBucket",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:GetBucketVersioning"
            ],
            "Resource": [
                "arn:aws:s3:::alphamind-emissions-data"
            ]
        }
    ]
}
