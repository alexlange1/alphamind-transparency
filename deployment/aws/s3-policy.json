{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AlphaMindEmissionsFullAccess",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:GetObjectVersion",
                "s3:PutObjectAcl",
                "s3:GetObjectAcl"
            ],
            "Resource": [
                "arn:aws:s3:::alphamind-emissions-data/*"
            ]
        },
        {
            "Sid": "AlphaMindEmissionsBucketAccess",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:GetBucketVersioning",
                "s3:PutBucketVersioning",
                "s3:GetBucketLifecycleConfiguration",
                "s3:PutBucketLifecycleConfiguration"
            ],
            "Resource": [
                "arn:aws:s3:::alphamind-emissions-data"
            ]
        }
    ]
}
