# ðŸš€ TAO20 Bittensor EVM Mainnet Configuration
# Copy this file to mainnet.env and fill in your values
# NEVER commit mainnet.env to git!

# Bittensor EVM Mainnet Settings
MAINNET_PRIVATE_KEY=your_mainnet_private_key_here
MAINNET_RPC_URL=https://api-bittensor-mainnet.n.dwellir.com/YOUR_API_KEY
MAINNET_CHAIN_ID=945
DWELLIR_API_KEY=your_dwellir_api_key_here

# Alternative RPC Providers (if available)
# MAINNET_RPC_URL=https://evm.bittensor.com
# MAINNET_RPC_URL=wss://api-bittensor-mainnet.n.dwellir.com/YOUR_API_KEY

# Production Settings
ENABLE_SOURCE_VERIFICATION=true
USE_GENERIC_NAMES=false
DEPLOYMENT_MODE=production

# Contract Names (Production)
PRODUCTION_CORE_NAME=TAO20CoreV2OracleFree
PRODUCTION_TOKEN_NAME=TAO20
PRODUCTION_VAULT_NAME=TAO20Vault

# Gas Settings
MAX_GAS_PRICE=50000000000  # 50 gwei
MAX_TOTAL_COST_TAO=1.0
GAS_BUFFER_MULTIPLIER=1.2

# Security
REQUIRE_CONFIRMATION=true
SAVE_DEPLOYMENT_LOG=true
AUTO_BACKUP_KEYS=true
ENABLE_MONITORING=true

# Monitoring & Alerts
DISCORD_WEBHOOK_URL=your_discord_webhook_here
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
SLACK_WEBHOOK_URL=your_slack_webhook_here

# Verification Settings
ETHERSCAN_API_KEY=not_needed_for_bittensor
ENABLE_CONTRACT_VERIFICATION=true
VERIFICATION_WAIT_TIME=60

# Emergency Settings
EMERGENCY_PAUSE_ENABLED=false
EMERGENCY_CONTACT=your_emergency_contact
MULTISIG_WALLET=your_multisig_address

# Performance Settings
DEPLOYMENT_TIMEOUT=300
CONFIRMATION_BLOCKS=3
RETRY_ATTEMPTS=3
