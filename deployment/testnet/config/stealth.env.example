# Copy this file to stealth.env and fill in your values
# NEVER commit stealth.env to git!

# Bittensor EVM Testnet Settings (Official)
TESTNET_PRIVATE_KEY=your_private_key_here
TESTNET_RPC_URL=https://test.chain.opentensor.ai
TESTNET_CHAIN_ID=945

# Privacy Settings
ENABLE_SOURCE_VERIFICATION=false
USE_GENERIC_NAMES=true
DEPLOYMENT_MODE=stealth

# Optional: Custom Contract Names (for extra privacy)
CUSTOM_CORE_NAME=IndexCore
CUSTOM_TOKEN_NAME=IDX20
CUSTOM_VAULT_NAME=AssetManager

# Monitoring (optional)
ENABLE_MONITORING=false
DISCORD_WEBHOOK_URL=
TELEGRAM_BOT_TOKEN=

# Deployment Limits
MAX_GAS_PRICE=20000000000  # 20 gwei
MAX_TOTAL_COST_TAO=0.1

# Security
REQUIRE_CONFIRMATION=true
SAVE_DEPLOYMENT_LOG=true
AUTO_BACKUP_KEYS=false
